{
  "vim_mode": true,
  "icon_theme": "Material Icon Theme",
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "dock": "right"
    // "version": "2"
  },
  "ui_font_size": 16,
  "buffer_font_size": 14,
  // "buffer_font_family": "SFMono Nerd Font Mono",
  // "buffer_font_family": "RobotoMono Nerd Font Mono",
  // "buffer_font_family": "Ubuntu Mono",
  "buffer_font_family": "FiraCode Nerd Font",
  // "buffer_font_family": "monospace",
  "buffer_font_weight": 400,
  "buffer_line_height": {
    "custom": 1.4
  },
  "collaboration_panel": {
    "button": false
  },
  "confirm_quit": true,
  "format_on_save": "off",
  "wrap_guides": [80],
  "tab_size": 2,
  "languages": {
    "Python": {
      "tab_size": 4
    },
    "C": {
      "tab_size": 4
    },
    "C++": {
      "tab_size": 4
    }
  },
  "terminal": {
    "font_size": 15
  },
  "theme": {
    "mode": "system",
    "light": "Ayu Dark",
    "dark": "macOS Classic Dark"
  }
}
